{
  "name": "geoterry",
  "version": "0.0.1",
  "private": true,
  "reactNativePermissionsIOS": [
    "LocationAlways",
    "LocationWhenInUse"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "prepare-ios": "cd ios && npx pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write \"App/**/*.+(ts|tsx|js|json|css|html|md)\"",
    "prepare": "husky install",
    "postinstall": "react-native setup-ios-permissions && pod-install"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/geolocation": "^3.0.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-native-button": "^3.0.1",
    "axios": "^1.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "i18next": "^23.2.8",
    "immer": "^10.0.2",
    "react": "18.2.0",
    "react-i18next": "^13.0.1",
    "react-native": "0.72.0",
    "react-native-button": "^3.1.0",
    "react-native-config": "^1.5.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-maps": "^1.7.1",
    "react-native-otp-textinput": "^1.1.1",
    "react-native-paper": "^5.10.3",
    "react-native-permissions": "^3.8.3",
    "react-native-reanimated": "^3.3.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-screens": "^3.22.1",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^13.13.0",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.2.3",
    "redux-thunk": "^2.4.2",
    "saga": "^4.0.0-alpha"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/config-conventional": "^17.6.6",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash": "^4.14.195",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "commitlint": "^17.6.6",
    "eslint": "^8.19.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "pod-install": "^0.1.38",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{ts,tsx,css}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
